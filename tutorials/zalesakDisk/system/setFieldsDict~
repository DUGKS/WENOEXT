FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
	volScalarFieldValue alpha.water 0
);

regions
(
    // Set cell values
    // (does zerogradient on boundaries)
    cylinderToCell
    {
        p1 (0.0 0.0 -0.03);

        p2 (0.0 0.0 0.03);

	radius 0.6;

        fieldValues
        (
            volScalarFieldValue alpha.water 1
        );
    }

    boxToCell
    {
        box (-0.1 -0.6 -0.03) (0.1 0.2 0.03);
        fieldValues 
        (
            volScalarFieldValue alpha.water 0
        );
    }

);

// ************************************************************************* //
