FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
libs            ("libWENOEXTDEV-2.3.x.so" "libOpenFOAM.so");

application     clsFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         1;

deltaT          0.001;

writeControl    adjustableRunTime;

writeInterval   0.01;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

purgeWrite 	0;

runTimeModifiable yes;

adjustTimeStep  on;

maxCo           0.5;
maxAlphaCo		0.3;

functions
